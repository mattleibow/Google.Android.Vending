<enum-field-mappings>
  <!--
  This example converts the constants Fragment_id, Fragment_name,
  and Fragment_tag from android.support.v4.app.FragmentActivity.FragmentTag
  to an enum called Android.Support.V4.App.FragmentTagType with values
  Id, Name, and Tag.
  
  <mapping jni-class="android/support/v4/app/FragmentActivity$FragmentTag" clr-enum-type="Android.Support.V4.App.FragmentTagType">
    <field jni-name="Fragment_name" clr-name="Name" value="0" />
    <field jni-name="Fragment_id" clr-name="Id" value="1" />
    <field jni-name="Fragment_tag" clr-name="Tag" value="2" />
  </mapping>
  -->

  <mapping jni-interface="com/google/android/vending/expansion/downloader/IDownloaderService" clr-enum-type="Google.Android.Vending.Expansion.Downloader.DownloaderServiceFlags">
    <field jni-name="FLAGS_NONE" clr-name="None" value="0" />
    <field jni-name="FLAGS_DOWNLOAD_OVER_CELLULAR" clr-name="DownloadOverCellular" value="1" />
  </mapping>

  <mapping jni-class="com/google/android/vending/expansion/downloader/impl/DownloaderService" clr-enum-type="Google.Android.Vending.Expansion.Downloader.DownloadServiceRequirement">
    <field jni-name="NO_DOWNLOAD_REQUIRED" clr-name="NoDownloadRequired" value="0" />
    <field jni-name="LVL_CHECK_REQUIRED" clr-name="LvlCheckRequired" value="1" />
    <field jni-name="DOWNLOAD_REQUIRED" clr-name="DownloadRequired" value="2" />
  </mapping>

  <mapping jni-interface="com/google/android/vending/expansion/downloader/IDownloaderClient" clr-enum-type="Google.Android.Vending.Expansion.Downloader.DownloaderClientState">
    <field jni-name="STATE_IDLE" clr-name="Idle" value="1" />
    <field jni-name="STATE_FETCHING_URL" clr-name="FetchingUrl" value="2" />
    <field jni-name="STATE_CONNECTING" clr-name="Connecting" value="3" />
    <field jni-name="STATE_DOWNLOADING" clr-name="Downloading" value="4" />
    <field jni-name="STATE_COMPLETED" clr-name="Completed" value="5" />
    <field jni-name="STATE_PAUSED_NETWORK_UNAVAILABLE" clr-name="PausedNetworkUnavailable" value="6" />
    <field jni-name="STATE_PAUSED_BY_REQUEST" clr-name="PausedByRequest" value="7" />
    <field jni-name="STATE_PAUSED_WIFI_DISABLED_NEED_CELLULAR_PERMISSION" clr-name="PausedWifiDisabledNeedCellularPermission" value="8" />
    <field jni-name="STATE_PAUSED_NEED_CELLULAR_PERMISSION" clr-name="PausedNeedCellularPermission" value="9" />
    <field jni-name="STATE_PAUSED_WIFI_DISABLED" clr-name="PausedWifiDisabled" value="10" />
    <field jni-name="STATE_PAUSED_NEED_WIFI" clr-name="PausedNeedWifi" value="11" />
    <field jni-name="STATE_PAUSED_ROAMING" clr-name="PausedRoaming" value="12" />
    <field jni-name="STATE_PAUSED_NETWORK_SETUP_FAILURE" clr-name="PausedNetworkSetupFailure" value="13" />
    <field jni-name="STATE_PAUSED_SDCARD_UNAVAILABLE" clr-name="PausedSdCardUnavailable" value="14" />
    <field jni-name="STATE_FAILED_UNLICENSED" clr-name="FailedUnlicensed" value="15" />
    <field jni-name="STATE_FAILED_FETCHING_URL" clr-name="FailedFetchingUrl" value="16" />
    <field jni-name="STATE_FAILED_SDCARD_FULL" clr-name="FailedSdCardFull" value="17" />
    <field jni-name="STATE_FAILED_CANCELED" clr-name="FailedCanceled" value="18" />
    <field jni-name="STATE_FAILED" clr-name="Failed" value="19" />
  </mapping>

</enum-field-mappings>
